    "react-icons/tfi": "https://deno.land/x/react_icons@0.1.3/tfi/mod.ts",
    "react-icons/vsc": "https://deno.land/x/react_icons@0.1.3/vsc/mod.ts",
    "clsx": "https://esm.sh/clsx"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "nodeModulesDir": "auto"
}
